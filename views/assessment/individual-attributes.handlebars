<form action="/assessment/individual-attributes" method="post">
    {{#each questions}}
    <div class="card bg-light mt-2">
        <div class="card">
            <div class="card-header">
                <p>
                    {{text}}
                </p>
            </div>
            <div class="card-body">
                {{#each answers}}
                <div class="radio">
                    <label>
                        <input type="radio" value="{{value}}" name="{{../_id}}" required> {{text}}
                    </label>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    {{/each}}
    <br>
    <div class="progress">
        <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="25" aria-valuemin="0"
            aria-valuemax="100" style="width: 25%">25%</div>
    </div>
    <br>
    <div class="text-center mb-2">
        <a href="/assessment/typing" class="btn btn-primary">Save and Continue</a>
    </div>
</form>